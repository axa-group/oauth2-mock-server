{
  "name": "oauth2-mock-server",
  "version": "5.0.0",
  "description": "OAuth 2 mock server",
  "keywords": [
    "oauth",
    "oauth2",
    "oauth 2",
    "mock",
    "server",
    "cli",
    "jwt",
    "oidc",
    "openid connect"
  ],
  "author": {
    "name": "Jorge Poveda",
    "email": "jorge.poveda@axa-assistance.es"
  },
  "license": "MIT",
  "engines": {
    "node": "^14.15 || ^16.13 || ^18",
    "yarn": "^1.15.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/axa-group/oauth2-mock-server.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "oauth2-mock-server": "./dist/oauth2-mock-server.js"
  },
  "files": [
    "CHANGELOG.md",
    "MIGRATION.md",
    "LICENSE.md",
    "README.md",
    "dist/**/*.*"
  ],
  "scripts": {
    "build:clean": "rimraf ./dist",
    "prebuild": "yarn build:clean",
    "build": "tsc -p ./tsconfig.build.json",
    "cleanup:testresults": "rimraf TestResults",
    "prelint": "tsc --noEmit",
    "lint": "eslint --cache --cache-location .cache/ --ext=.js,.ts src test --max-warnings 0",
    "prepack": "yarn build --tsBuildInfoFile null --incremental false",
    "pretest": "yarn cleanup:testresults && yarn lint",
    "test": "yarn jest"
  },
  "dependencies": {
    "basic-auth": "^2.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jose": "^4.8.1",
    "lodash.isplainobject": "^4.0.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/basic-auth": "^1.1.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.3",
    "@types/lodash.isplainobject": "^4.0.7",
    "@types/node": "^14.18.21",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "jest-junit": "^14.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "@types/node": "^14"
  }
}
